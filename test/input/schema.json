{
    "blocks": {
        "root": {
            "defaultTag": "paragraph",
            "props": [
                {
                    "name": "content",
                    "content": [
                        {
                            "tag": "paragraph",
                            "cardinality": "zeroOrMore"
                        },
                        {
                            "tag": "list",
                            "cardinality": "zeroOrMore"
                        },
                        {
                            "tag": "section",
                            "cardinality": "zeroOrMore"
                        },
                        {
                            "tag": "code",
                            "cardinality": "zeroOrMore"
                        }
                    ]
                }
            ]
        },
        "list": {
            "head": {
                "name": "title",
                "content": [
                    "link",
                    "bold",
                    "inline",
                    "code"
                ]
            },
            "defaultTag": "paragraph",
            "props": [
                {
                    "name": "items",
                    "content": [
                        {
                            "tag": "paragraph",
                            "cardinality": "zeroOrMore"
                        }
                    ]
                }
            ]
        },
        "paragraph": {
            "head": {
                "name": "title",
                "content": [
                    "link",
                    "bold",
                    "inline",
                    "code"
                ]
            },
            "props": []
        },
        "section": {
            "head": {
                "name": "title",
                "content": [
                    "link",
                    "bold",
                    "inline",
                    "code"
                ]
            },
            "defaultTag": "paragraph",
            "props": [
                {
                    "name": "content",
                    "content": [
                        {
                            "tag": "paragraph",
                            "cardinality": "zeroOrMore"
                        },
                        {
                            "tag": "section",
                            "cardinality": "zeroOrMore"
                        },
                        {
                            "tag": "code",
                            "cardinality": "zeroOrMore"
                        }
                    ]
                }
            ]
        },
        "code": {
            "props": [
                {
                    "name": "content",
                    "raw": true
                }
            ]
        }
    },
    "inline": {
        "link": {
            "sugar": {
                "start": "[",
                "separator": "](",
                "end": ")"
            },
            "props": [
                {
                    "name": "url",
                    "raw": true
                },
                {
                    "name": "text",
                    "content": [
                        "bold"
                    ]
                }
            ]
        },
        "bold": {
            "sugar": {
                "start": "*",
                "end": "*"
            },
            "props": [
                {
                    "name": "text",
                    "content": [
                        "link"
                    ]
                }
            ]
        },
        "inline": {
            "props": [
                {
                    "name": "inlineContent",
                    "content": [
                        "link",
                        "bold",
                        "inline",
                        "code"
                    ]
                }
            ]
        },
        "code": {
            "props": [
                {
                    "name": "content",
                    "raw": true
                }
            ],
            "sugar": {
                "start": "`",
                "end": "`"
            }
        }
    }
}
